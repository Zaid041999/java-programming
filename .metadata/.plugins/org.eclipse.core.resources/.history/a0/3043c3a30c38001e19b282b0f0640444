package progamming.array_programs_7;

public class Missing_Element_In_The_Array_3 {

	public static void main(String[] args) {
		int[] a= {1,4,5,2,6};
		int sum1=0;
		int sum2=0;
		int n = a.length+1;
		sum1 = n * (n+1)/2;
		for(int i = 0;i<a.length;i++) {
			sum2 =sum2+a[i];
		}
		System.out.println(sum1-sum2);
	}

}
