package progamming.array_programs_7;

public class Linear_Search_Algorithm_8 {

	public static void main(String[] args) {
		int[] a = {2,3,1,4,5,6};
		int item = 7;
		int temp = 0;//for if item not present temp value will not be incremented
		for (int i = 0; i < a.length; i++) {
			if(a[i]==item) {
				System.out.println("item is present on "+i+" index position");
				temp=temp+1;
			}
		}
		if(temp==0) {
			System.out.println("item is not present");
		}

	}

}
